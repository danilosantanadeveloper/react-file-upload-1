{
  "name": "react-file-upload",
  "version": "1.0.0",
  "description": "React full stack image upload",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prestart": "mongod --dbpath='./db'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndrewJBateman/react-file-upload.git"
  },
  "keywords": [
    "react"
  ],
  "author": "ABateman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AndrewJBateman/react-file-upload/issues"
  },
  "homepage": "https://github.com/AndrewJBateman/react-file-upload#readme",
  "dependencies": {
    "express": "^4.17.3",
    "express-fileupload": "^1.3.1",
    "mongodb": "^4.8.0",
    "mongoose": "^6.4.4",
    "multer": "^1.4.5-lts.1",
    "promised-csv": "^1.0.1"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  }
}
